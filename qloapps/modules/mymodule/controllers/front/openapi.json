{
  "openapi": "3.0.0",
  "info": {
    "title": "MyModule API",
    "version": "1.0.0",
    "description": "API documentation for MyModule endpoints"
  },
  "paths": {
    "/module/mymodule/orders": {
      "get": {
        "summary": "Get list of orders",
        "parameters": [
          {
            "in": "query",
            "name": "token",
            "schema": {
              "type": "string"
            },
            "required": true,
            "example": "my_static_token"
          }
        ],
        "responses": {
          "200": {
            "description": "A list of orders",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "orders": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "string",
                            "example": "1"
                          },
                          "reference": {
                            "type": "string",
                            "example": "TVULXTAFZ"
                          },
                          "total_paid": {
                            "type": "string",
                            "example": "8400.000000"
                          },
                          "date_add": {
                            "type": "string",
                            "format": "date-time",
                            "example": "2024-05-24 14:19:15"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/module/mymodule/order": {
      "get": {
        "summary": "Get order details",
        "parameters": [
          {
            "in": "query",
            "name": "token",
            "schema": {
              "type": "string"
            },
            "required": true,
            "example": "my_static_token"
          },
          {
            "in": "query",
            "name": "id",
            "schema": {
              "type": "integer"
            },
            "required": true,
            "example": 1
          }
        ],
        "responses": {
          "200": {
            "description": "Order details",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "integer",
                      "example": 1
                    },
                    "reference": {
                      "type": "string",
                      "example": "TVULXTAFZ"
                    },
                    "total_paid": {
                      "type": "string",
                      "example": "8400.000000"
                    },
                    "date_add": {
                      "type": "string",
                      "format": "date-time",
                      "example": "2024-05-24 14:19:15"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/module/mymodule/products": {
      "get": {
        "summary": "Get list of products",
        "parameters": [
          {
            "in": "query",
            "name": "token",
            "schema": {
              "type": "string"
            },
            "required": true,
            "example": "my_static_token"
          }
        ],
        "responses": {
          "200": {
            "description": "A list of products",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "products": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "string",
                            "example": "1"
                          },
                          "name": {
                            "type": "string",
                            "example": "Budget cabin - TEST"
                          },
                          "description": {
                            "type": "string",
                            "example": "description long"
                          },
                          "description_short": {
                            "type": "string",
                            "example": "description_short"
                          },
                          "price": {
                            "type": "string",
                            "example": "1000.000000"
                          },
                          "quantity": {
                            "type": "string",
                            "example": "0"
                          },
                          "new": {
                            "type": "string",
                            "example": null
                          },
                          "active": {
                            "type": "string",
                            "example": "1"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/module/mymodule/product": {
      "get": {
        "summary": "Get product details",
        "parameters": [
          {
            "in": "query",
            "name": "token",
            "schema": {
              "type": "string"
            },
            "required": true,
            "example": "my_static_token"
          },
          {
            "in": "query",
            "name": "id",
            "schema": {
              "type": "integer"
            },
            "required": true,
            "example": 1
          }
        ],
        "responses": {
          "200": {
            "description": "Product details",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "integer",
                      "example": 1
                    },
                    "name": {
                      "type": "string",
                      "example": "Budget cabin - TEST"
                    },
                    "description": {
                      "type": "string",
                      "example": "description long"
                    },
                    "description_short": {
                      "type": "string",
                      "example": "description_short"
                    },
                    "price": {
                      "type": "integer",
                      "example": 1000
                    },
                    "quantity": {
                      "type": "integer",
                      "example": 999999999
                    },
                    "new": {
                      "type": "boolean",
                      "example": false
                    },
                    "active": {
                      "type": "string",
                      "example": "1"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Update product details",
        "parameters": [
          {
            "in": "query",
            "name": "token",
            "schema": {
              "type": "string"
            },
            "required": true,
            "example": "my_static_token"
          },
          {
            "in": "query",
            "name": "id",
            "schema": {
              "type": "integer"
            },
            "required": true,
            "example": 1
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "integer",
                    "example": 1
                  },
                  "name": {
                    "type": "string",
                    "example": "Budget cabin - TEST"
                  },
                  "description": {
                    "type": "string",
                    "example": "description long"
                  },
                  "description_short": {
                    "type": "string",
                    "example": "description_short"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Product updated successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "integer",
                      "example": 1
                    },
                    "name": {
                      "type": "string",
                      "example": "Budget cabin - TEST"
                    },
                    "description": {
                      "type": "string",
                      "example": "description long"
                    },
                    "description_short": {
                      "type": "string",
                      "example": "description_short"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}